{
  "login": {
    "title": "Login",
    "switchPage": [{
      "text": "not register ?",
      "to": "register"
    }, {
      "text": "refresh password",
      "to": "refresh"
    }],
    "button": "Sign in",
    "fields": ["nameEmail","password"]
  },
  "register": {
    "title": "Register",
    "switchPage": [{
      "text": "have account ?",
      "to": "login"
    }],
    "button": "Sign up",
    "fields": ["name", "email", "password", "repeat_password"]
  },
  "refresh": {
    "title": "Refresh password",
    "button": "refresh",
    "switchPage": [{
      "text": "not register ?",
      "to": "register"
    }, {
      "text": "have account ?",
      "to": "login"
    }],
    "fields": ["email"]
  }
}